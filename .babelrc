  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": ["@babel/transform-runtime"],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "react-hot-loader/babel",
        "babel-plugin-transform-object-assign"
      ]
    }
  }
}